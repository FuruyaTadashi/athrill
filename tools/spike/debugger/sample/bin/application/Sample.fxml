<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <AnchorPane prefHeight="687.0" prefWidth="671.0" BorderPane.alignment="CENTER">
         <children>
            <TabPane layoutY="7.0" prefHeight="774.0" prefWidth="671.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab fx:id="fileName" text="Untitled Tab 1">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="600.0" prefWidth="700.0">
                           <children>
                              <SplitPane layoutX="10.0" layoutY="20.0" prefHeight="520.0" prefWidth="640.0">
                                <items>
                                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="520.0" prefWidth="640.0">
                                       <children>
                                          <TextFlow layoutX="30.0" layoutY="30.0" prefHeight="400.0" prefWidth="600.0">
                                             <children>
                                                <TextArea fx:id="lineArea" editable="false" prefHeight="380.0" prefWidth="70.0">
                                                   <font>
                                                      <Font name="System Bold" size="13.0" />
                                                   </font></TextArea>
                                                <TextArea fx:id="codeArea" editable="false" onMouseClicked="#codeAreaonMouseClicked" prefHeight="380" prefWidth="500.0">
                                                   <font>
                                                      <Font name="System Bold" size="13.0" />
                                                   </font></TextArea>
                                             </children>
                                          </TextFlow>
                                       </children>
                                    </AnchorPane>
                                </items>
                              </SplitPane>
                           </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
